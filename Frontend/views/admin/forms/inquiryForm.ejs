

<div class="container mx-auto my-8">
    <h1 class="text-[#CA9650] text-3xl font-bold text-center mb-8">Inquiry Submissions</h1>

    <div class="bg-white shadow-md rounded-lg p-6">
        <div class="table-responsive">
            <table class="min-w-full leading-normal table-auto">
                <thead>
                    <tr>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Organisation Name</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Contact No</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Address</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Brand</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Contact Person</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">GST</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Monthly Turnover</th>
                        <th class="px-4 py-2 border-b-2 border-gray-200 bg-gray-100 text-left text-sm font-semibold text-gray-700 uppercase">Years in Business</th>
                    </tr>
                </thead>
                <tbody>
                    <% inquiries.forEach(function(inquiry) { %>
                    <tr>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.orgName %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.contactNo %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.address %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.brand %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.contactPerson %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.gst %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.monthlyTurnover %></td>
                        <td class="px-4 py-2 border-b border-gray-200 text-sm"><%= inquiry.yearsInBusiness %></td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>


